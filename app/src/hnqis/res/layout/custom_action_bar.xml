<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="56.0dip"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_marginLeft="30dp">
    <org.eyeseetea.malariacare.views.CustomTextView android:id="@+id/action_bar_multititle_title"
        android:layout_height="28dip"
        android:layout_width="fill_parent"
        android:width="0dp"
        app:tFontName="@string/light_font"
        android:textSize="20dip"/>
    <org.eyeseetea.malariacare.views.CustomTextView android:id="@+id/action_bar_multititle_subtitle"
        android:layout_height="28dip"
        android:layout_width="fill_parent"
        android:width="0dp"
        app:tFontName="@string/light_font"
        android:textSize="20dip"/>
    <org.eyeseetea.malariacare.views.CustomTextView android:id="@+id/action_bar_user"
        android:layout_height="28dip"
        android:layout_width="fill_parent"
        android:visibility="gone"
        android:width="0dp"
        app:tFontName="@string/light_font"
        android:textSize="20dip"/>
</LinearLayout>