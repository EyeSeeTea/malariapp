<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">


    <GridLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:columnCount="1"
        android:rowCount="2"
        android:id="@+id/supervisor">

        <org.eyeseetea.malariacare.views.TextCard
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="@string/iqatab_info_supervisor"
            android:id="@+id/supervisorTitle"
            android:textStyle="bold"
            android:paddingBottom="20dip"
            app:tDimension="@string/font_size_level2"
            app:tFontName="@string/condensed_font_name" />

        <TableLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:id="@+id/supervisorTable"
            android:tag="IQEQA1"
            android:layout_gravity="center">

            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:id="@+id/supervisorHeader"
                android:layout_gravity="center"
                android:background="@drawable/background_odd">

                <Space
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content" />

                <org.eyeseetea.malariacare.views.TextCard
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="@string/adherence_title_test_results"
                    android:id="@+id/supervisorResults"
                    android:background="@drawable/background_header"
                    android:textColor="#ffffff"
                    android:lines="2"
                    android:gravity="center"
                    android:padding="5dip"
                    android:layout_weight="1"
                    app:tDimension="@string/font_size_level2"
                    app:tFontName="@string/condensed_font_name" />

                <org.eyeseetea.malariacare.views.TextCard
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="@string/iqatab_info_parasites"
                    android:id="@+id/supervisorParasites"
                    android:background="@drawable/background_header"
                    android:textColor="#ffffff"
                    android:gravity="center"
                    android:lines="2"
                    android:padding="5dip"
                    android:layout_weight="1"
                    app:tDimension="@string/font_size_level2"
                    app:tFontName="@string/condensed_font_name" />

                <org.eyeseetea.malariacare.views.TextCard
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="@string/iqatab_info_species"
                    android:id="@+id/supervisorSpecies"
                    android:background="@drawable/background_header"
                    android:textColor="#ffffff"
                    android:lines="2"
                    android:gravity="center"
                    android:padding="5dip"
                    android:layout_weight="1"
                    app:tDimension="@string/font_size_level2"
                    app:tFontName="@string/condensed_font_name" />

            </TableRow>
        </TableLayout>
    </GridLayout>

    <Space
