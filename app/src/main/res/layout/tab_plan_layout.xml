<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"

    android:id="@+id/tab_plan_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <org.eyeseetea.malariacare.views.filters.OrgUnitProgramFilterView
            android:id="@+id/plan_org_unit_program_filter_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

        <FrameLayout
            android:id="@+id/dashboard_planning_init"
            android:layout_width="match_parent"
            android:background="@color/plan_listview_background"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">
            </LinearLayout>
        </FrameLayout>

        <FrameLayout
            android:id="@+id/dashboard_planning_orgunit"
            android:layout_width="match_parent"
            android:background="@color/plan_listview_background"
            android:layout_height="match_parent"
            android:orientation="vertical">
        </FrameLayout>
    </LinearLayout>


</LinearLayout>